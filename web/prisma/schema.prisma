generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model carts {
  id         BigInt       @id @default(autoincrement()) @db.UnsignedBigInt
  user_id    Int?
  created_at DateTime?    @db.Timestamp(0)
  updated_at DateTime?    @db.Timestamp(0)
  cart_items cart_items[]
}

model cart_items {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  cart_id    BigInt    @db.UnsignedBigInt
  product_id Int?      @db.UnsignedInt
  sku        Int
  size       String?   @db.VarChar(20)
  quantity   Int
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)

  carts   carts     @relation(fields: [cart_id], references: [id], onDelete: Cascade)
  product products? @relation(fields: [product_id], references: [id])
  item    items     @relation("ItemCartItems", fields: [sku], references: [sku])

  @@unique([cart_id, sku])
}

model addons {
  id              BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  name            String    @db.VarChar(255)
  keyword         String?
  type            String?
  uninstall_files String?   @db.Text
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
}

model admin_languages {
  id         Int     @id @default(autoincrement())
  is_default Boolean @default(false)
  language   String  @db.VarChar(100)
  file       String  @db.VarChar(100)
  name       String  @db.VarChar(100)
  rtl        Boolean @default(false)
}

model admin_user_conversations {
  id           Int                            @id @default(autoincrement())
  subject      String
  user_id      Int
  message      String                         @db.Text
  created_at   DateTime?                      @db.Timestamp(0)
  updated_at   DateTime?                      @db.Timestamp(0)
  type         admin_user_conversations_type?
  order_number String?                        @db.Text
}

model admin_user_messages {
  id              Int       @id @default(autoincrement())
  conversation_id Int
  message         String    @db.Text
  user_id         Int?
  created_at      DateTime? @db.Timestamp(0)
  updated_at      DateTime? @db.Timestamp(0)
}

model admins {
  id             Int       @id @default(autoincrement()) @db.UnsignedInt
  name           String
  email          String    @unique(map: "admins_email_unique")
  phone          String
  role_id        Int       @default(0)
  photo          String?
  password       String
  status         Int       @default(1) @db.TinyInt
  remember_token String?   @db.VarChar(100)
  email_token    String?   @db.VarChar(100)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)
  shop_name      String?   @db.Text
}

model affliate_bonuses {
  id             BigInt    @id @default(autoincrement())
  refer_id       Int
  bonus          Float     @default(0)
  type           String    @db.VarChar(255)
  user_id        Int
  customer_email String    @db.VarChar(255)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)
}

model arrival_sections {
  id         Int       @id @default(autoincrement())
  title      String    @db.VarChar(500)
  header     String    @db.VarChar(500)
  photo      String    @db.VarChar(300)
  up_sale    String?   @db.VarChar(255)
  url        String?   @db.Text
  created_at DateTime  @default(now()) @db.Timestamp(6)
  updated_at DateTime? @db.Timestamp(6)
}

model attribute_options {
  id           Int       @id @default(autoincrement())
  attribute_id Int?
  name         String?   @db.VarChar(255)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model attributes {
  id                Int       @id @default(autoincrement())
  attributable_id   Int?
  attributable_type String?   @db.VarChar(255)
  name              String?   @db.VarChar(255)
  input_name        String?   @db.VarChar(255)
  price_status      Int       @default(1)
  details_status    Int       @default(1)
  created_at        DateTime? @db.Timestamp(0)
  updated_at        DateTime? @db.Timestamp(0)
}

model banners {
  id    Int          @id @default(autoincrement())
  photo String?
  link  String?
  type  banners_type
  title String?      @db.VarChar(255)
  text  String?      @db.VarChar(255)
}

model blog_categories {
  id   Int    @id @default(autoincrement())
  name String
  slug String
}

model blogs {
  id               Int       @id @default(autoincrement()) @db.UnsignedInt
  category_id      Int
  title            String
  slug             String    @db.VarChar(255)
  details          String    @db.Text
  photo            String?
  source           String
